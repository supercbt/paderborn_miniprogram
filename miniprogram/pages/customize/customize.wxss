.UCenter-bg {
  background-image: url(https://686f-homework-7gii7l2555f9fe8e-1301606952.tcb.qcloud.la/webp.jpg?sign=28d9b0ad7890982e43cd81209a5a304c&t=1619009142);
  background-size: cover;
  height: 550rpx;
  display: flex;
  justify-content: center;
  padding-top: 40rpx;
  overflow: hidden;
  position: relative;
  flex-direction: column;
  align-items: center;
  color: #fff;
  font-weight: 300;
  text-shadow: 0 0 3px rgba(0, 0, 0, 0.3);
}

.UCenter-bg text {
  opacity: 0.8;
}

.UCenter-bg image {
  width: 200rpx;
  height: 200rpx;
}

.UCenter-bg .gif-wave{
  position: absolute;
  width: 100%;
  bottom: 0;
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  left: 0;
  z-index: 99;
  mix-blend-mode: screen;  
  height: 100rpx;   
}

map,.mapBox{
  width: 750rpx;
  height: 300rpx;
}
.cu-item::after{
  border-bottom:none
}

page{
  background-color:white
}

.card{
  box-shadow: 0 4rpx 32rpx 0 rgba(74,96,142,0.16) ;
  margin-bottom: 40rpx;
}